{
    "name": "featurit/featurit-sdk-laravel",
    "description": "Laravel SDK for FeaturIT",
    "keywords": ["featurit", "feature flag", "feature toggle", "user targeting", "a b testing", "feature", "feature management", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "FeaturIT",
            "email": "featurit.tech@gmail.com",
            "homepage": "https://www.featurit.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Featurit\\Client\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Featurit\\Client\\Larvel\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require": {
        "php": "^8.0.2",
        "ext-json": "*",
        "featurit/featurit-sdk-php": "^0.9.0",
        "illuminate/support": "^5.1 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0"
    },
    "require-dev": {
        "laravel/framework": "^11.0",
        "orchestra/testbench": "^9.0",
        "php-http/guzzle7-adapter": "^1.0",
        "phpunit/phpunit": "^10.0"
    },
    "suggest": {
        "laravel/framework": "To test the Laravel bindings"
    }
}
